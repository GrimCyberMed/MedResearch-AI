{
  "name": "medresearch-ai",
  "version": "4.2.0",
  "description": "MedResearch AI - Multi-Agent System for Systematic Reviews with Advanced Biostatistics & Memory",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "node --test",
    "test:meta": "node tests/test-meta-analysis.js",
    "test:databases": "node tests/test-databases.js",
    "test:plagiarism": "node tests/test-plagiarism.js",
    "test:all": "npm run test:databases && npm run test:plagiarism",
    "dashboard": "node tests/dashboard.js",
    "dashboard:watch": "node tests/dashboard.js --watch",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.{ts,json,md}\""
  },
  "keywords": [
    "medical-research",
    "systematic-review",
    "meta-analysis",
    "biostatistics",
    "statistical-analysis",
    "power-analysis",
    "ai-agents",
    "opencode",
    "prisma-2020"
  ],
  "author": "MedResearch AI Team",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.3.1",
    "fast-xml-parser": "^4.3.2",
    "node-cache": "^5.1.2",
    "sql.js": "^1.13.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/sql.js": "^1.4.9",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
